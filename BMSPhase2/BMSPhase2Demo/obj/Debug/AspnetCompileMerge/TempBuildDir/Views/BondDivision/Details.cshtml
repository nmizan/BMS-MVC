@model BMSPhase2Demo.Models.BONDDIVISION

@{
    ViewBag.Title = "Details";
}
<style type="text/css">
    table, th, td {
        border: 0px;
        border-collapse: collapse;
        padding-left: 10px;
        padding-right: 10px;
        padding-top: 10px;
        padding-bottom: 10px;
        font-size: 15px;
        margin-left :270px;
    }

    .textBOxSize {
        width: 220px;
    }
</style>
<p style =" margin-left :400px;font-size :25px; ">Bond Division Information</p>
<h2 style ="margin-left :270px;">Details</h2>

<fieldset>
    <legend>BONDDIVISION</legend>


    <table>

        <tr>
            <td>
                Bond Division SL No:
                @Html.HiddenFor(model => model.BONDDIVISIONSLNO)
            </td>
            <td>
                @Html.TextBoxFor(model => model.BONDDIVISIONSLNO, new { @readonly = "readonly" })
            </td>
        </tr>
        <tr>
            <td>
                Bond Division Name:
            </td>
            <td> @Html.TextBoxFor(model => model.BONDDIVISIONNAME, new { @readonly = "readonly" })</td>
        </tr>
        <tr>
            <td>
                Bond Division Name BG:
            </td>
            <td>
                @Html.TextBoxFor(model => model.BONDDIVISIONNAMEBG, new { @readonly = "readonly" })
            </td>
        </tr>
        <tr>
            <td>Input By:</td>
            <td>@Html.TextBoxFor(model => model.INPUTBY, new { @readonly = "readonly" })</td>
        </tr>
        <tr>
            <td>Input date:</td>
            <td>@Html.TextBoxFor(model => model.INPUTDATE, new { @readonly = "readonly" })</td>
        </tr>
        <tr>
            <td>Modify By:</td>
            <td>@Html.TextBoxFor(model => model.MODIFIEDBY, new { @readonly = "readonly" })</td>
        </tr>
        <tr>
            <td>Modified Date:</td>
            <td>@Html.TextBoxFor(model => model.MODIFYDATE, new { @readonly = "readonly" })</td>
        </tr>
    </table>
</fieldset>
<p style ="margin-left :270px;">
    @Html.ActionLink("Edit", "Edit", new { id = Model.BONDDIVISIONSLNO }) |
    @Html.ActionLink("Back to List", "Index")
</p>

